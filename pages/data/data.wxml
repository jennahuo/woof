<view class="page__container">
  <view class="subheader">{{test}}</view>
  <view bindtap="navigateToCountry" class="image"></view>


  <!-- confirmed cases -->
  <view class="container">
    <view class="name">TOTAL CASES </view>
    <view class="data">{{results.cases}}</view>
  </view>

  <!-- cured cases -->
  <view class="container">
    <view class="name">TOTAL DEATH </view>
    <view class="data">{{results.deaths}}</view>
  </view>

  <!-- deceased cases -->
  <view class="container">
    <view class="name">TOTAL RECOVERED </view>
    <view class="data">{{results.recovered}}</view>
  </view>

  <button bindtap="fetchData" class="btn">FETCH DATA</button>
</view>